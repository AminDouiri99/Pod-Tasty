                    </div>
                </div>
               {% if app.user == null %}
                <div class="col" >
                    <div class="header_content d-flex flex-row align-items-right justify-content-start trans_400">
                        <nav class="main_nav">
                            <div class="buttons" style="padding-top: 10px;width: 100%;margin-left: 200%">
                                <div class="navbar-item">
                                    <a class="button is-primary has-text-white logregButt" href="{{ path('app_login') }}">
                                        <strong>Login</strong>

                                    </a>


                                </div>
                                <a class="button is-primary has-text-white logregButt" href="{{ path('register') }}">
                                    <strong>Sign up</strong>
                                </a>

                            </div>

                        </nav>
                    </div>
                </div>
                {% endif %}
                {% if app.user  %}
                <input style="display: none" id="userId" value="{{ app.user.id }}"/>

                <div style="position: absolute;top:1px;left:89%">
                    <div class="dropdown">
                        <div style="margin-top: 20px;padding-top: 12px;position: static">
                            <a role="button" id="notifDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="notificationaa"><i style="font-size: 30px;cursor: pointer;color: white" id="notifButton"  class="fa fa-bell"></i>
                                <span style="position: absolute" class="badge" id="notifnotviewed">{% if app.user.notificationList|length != 0 %}{{ app.user.notificationList|length }}{% endif %}</span>
                            </a>
                            <div style="margin-top: 45px;margin-right:250px;min-height:300px;max-height: 600px;overflow: auto"  class="dropdown-menu" aria-labelledby="notifDropdown">

                                {% if app.user.notificationList|length == 0  %}
                                <a class="dropdown-item" style="padding-top:130px;padding-left: 70px;font-weight: bolder;font-size: 17px"><span>No notifications</span></a>
                                {% endif %}
                            {% if app.user.notificationList|length != 0  %}
                                <div id="allNotifs">
                            {% for n in app.user.notificationList %}
                                <div id="allNotifs">
                                <div style="padding:10px;margin:10px" id="notif{{ n.id }}">
                                    <span style="font-size: 17px;font-weight: bolder">{{ n.notificationTitle}}</span><br>
                                    <span style="margin-left: 15px">{{ n.notificationDescription| raw }}</span><br>
                                    <span style="float: right">{{ n.notificationDate().format("D/M/Y")}}</span>
                                </div>
                                </div>
                            {% endfor %}
                                </div>
                            {% endif %}
                        </div>
                        </div>
                    </div>
                    </div>
                    <div class="dropdown" style="margin-right: 40px">
                        <div style="padding-top: 15px;overflow:hidden;margin-top: 27px;height: 50px !important;z-index: 999999999999999;position: static !important;">
                            <a style="float:right;" role="button" id="profileDropDown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <img style="height: 40px;width:55px;cursor: pointer;margin-right: -15px;" id="notifButton" class="avatar  rounded-circle " src="

                                {% if app.user.userInfoId.getUserImage() is not null %}
                                {{ asset('/Files/usersFiles/'~app.user.userInfoId.getUserImage() ) }}
                                {% else %}
                                {{ asset('Files/usersFiles/avatar.jpg') }}
                                {% endif %}">
                            </a>

                            <div style="margin-top: 36px;width:20px !important;;margin-right: -120px;"  class="dropdown-menu" aria-labelledby="profileDropDown">
                                <a class="dropdown-item" style="font-weight: bolder" href="/profile/{{ app.user.id }}">Profile</a>
                                <hr>
                                <a class="dropdown-item" style="font-weight: bolder" href="{{ path('app_logout') }}">Sign out</a>
                            </div>
